<h1>Listing ratings</h1>

<table>
  <thead>
    <tr>
      <th>Score</th>
      <th>Subject type</th>
      <th>Subject</th>
      <th>Creator</th>
      <th>Current</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @ratings.each do |rating| %>
      <tr>
        <td><%= rating.score %></td>
        <td><%= rating.subject_type %></td>
        <td><%= rating.subject_id %></td>
        <td><%= rating.creator_id %></td>
        <td><%= rating.current %></td>
        <td><%= link_to 'Show', rating %></td>
        <td><%= link_to 'Edit', edit_rating_path(rating) %></td>
        <td><%= link_to 'Destroy', rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rating', new_rating_path %>
